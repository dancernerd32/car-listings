<h1><%= @manufacturer.name %></h1>

<ul>
  <% @cars.each do |car| %>
    <li>
      <ul>
        <li>Year: <%= car.year %></li>
        <li>Color: <%= car.color %></li>
        <li>Mileage: <%= car.mileage %> miles</li>
        <li>Description: <%= car.description %></li>
      </ul>
    </li>
  <% end %>
</ul>
